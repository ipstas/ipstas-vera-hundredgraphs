<?xml version="1.0"?>
<!-- S_HundredGraphs1.xml -->
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="Yes">
			<name>API</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>API</shortCode>
		</stateVariable>
		<stateVariable sendEvents="Yes">
			<name>Interval</name>
			<dataType>string</dataType>
			<defaultValue>600</defaultValue>
			<shortCode>Interval</shortCode>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>Enabled</name>
			<dataType>string</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>Enabled</shortCode>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>lastRun</name>
			<dataType>string</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>lastRun</shortCode>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>SetAPI</name>
			<argumentList>
				<argument>
					<name>newAPIValue</name>
					<relatedStateVariable>API</relatedStateVariable>
					<direction>in</direction>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAPI</name>
			<argumentList>
				<argument>
					<name>ResultValue</name>
					<relatedStateVariable>API</relatedStateVariable>
					<direction>out</direction>
					<retval/>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetInterval</name>
			<argumentList>
				<argument>
					<name>newIntervalValue</name>
					<relatedStateVariable>Interval</relatedStateVariable>
					<direction>in</direction>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetInterval</name>
			<argumentList>
				<argument>
					<name>ResultValue</name>
					<relatedStateVariable>Interval</relatedStateVariable>
					<direction>out</direction>
					<retval/>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetEnabled</name>
			<argumentList>
				<argument>
					<name>newEnabledValue</name>
					<direction>in</direction>
					<relatedStateVariable>Enabled</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetDebug</name>
			<argumentList>
				<argument>
					<name>debug</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_boolean</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>